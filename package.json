{
  "name": "express-mongo-error-handler",
  "version": "1.2.0",
  "description": "Error handling middleware for MERN backend applications using Express, MongoDB and JWT.",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "index.d.ts",
    "index.d.cts",
    "LICENSE",
    "README.md",
    "SECURITY.md",
    "CONTRIBUTING.md",
    "package.json"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build && npm test",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jordanleal12/express-mongo-error-handler.git"
  },
  "keywords": [
    "express",
    "mongodb",
    "mongoose",
    "jwt",
    "error handling",
    "middleware",
    "mern"
  ],
  "author": "Jordan Leal <https://github.com/jordanleal12>",
  "license": "MIT",
  "type": "module",
  "bugs": {
    "url": "https://github.com/jordanleal12/express-mongo-error-handler/issues"
  },
  "homepage": "https://github.com/jordanleal12/express-mongo-error-handler#readme",
  "devDependencies": {
    "jest": "^30.2.0",
    "supertest": "^7.1.4",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
